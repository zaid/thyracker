#header
  #header-logo
    %span= link_to "Thyracker", root_path
  #header-navigation
    #inner-navigation
      - if signed_in?
        %ul
          %li= link_to "Samples", samples_path
          %li= link_to "Charts", charts_path
  - unless signed_in?
    #header-signin
      = render 'sessions/signin_form'
  - else
    #header-signout
      = render 'sessions/signout_form'
